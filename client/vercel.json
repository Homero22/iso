{
    "version": 2,
    "builds": [
      {
        "src": "dist/client/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "dist/client/index.html"
      }
    ],
    "headers": [
        {
          "source": "/api/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        }
      ]
  }